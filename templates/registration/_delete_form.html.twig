<form method="post" class="form-delete-utilisateur" action="{{ path('app_user_delete', {'id': user.id}) }}" onsubmit="return confirm('Êtes vous sûre de vouloir supprimer cet utilisateur ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn btn-delete-user">Supprimer</button>
</form>