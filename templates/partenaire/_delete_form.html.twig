<form method="post" class="form-delete-partenaire" action="{{ path('app_partenaire_delete', {'id': partenaire.id}) }}" onsubmit="return confirm('Êtes-vous sûre de vouloir supprimer ce partenaire?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ partenaire.id) }}">
    <button class="btn btn-delete-partenaire">Supprimer</button>
</form>
